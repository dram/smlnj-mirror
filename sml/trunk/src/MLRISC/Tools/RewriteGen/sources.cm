                 bind:(anchor:MLTREE.cm value:$SMLNJ-MLRISC)
                 bind:(anchor:MLRISC.cm value:$SMLNJ-MLRISC))

	../SourceMap/sources.cm 
	../FakeSMLAst/sources.cm (* the fake SML abstract syntax *)
	../PP/sources.cm         (* pretty printer utilities *)
	../Parser/sources.cm     (* parser *)

	poly-gen.sig
	poly-gen.sml
	rewrite-gen.sig
	rewrite-gen.sml
	glue.sml
