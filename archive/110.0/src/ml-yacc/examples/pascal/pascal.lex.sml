fun makeLexer yyinput =
let	val yygone0=1
	val yyb = ref "\n" 		(* buffer *)
	val yybl = ref 1		(*buffer length *)
	val yybufpos = ref 1		(* location of next character to use *)
	val yygone = ref yygone0	(* position in file of beginning of buffer *)
	val yydone = ref false		(* eof found yet? *)
	val yybegin = ref 1		(*Current 'start state' for lexer *)

	val YYBEGIN = fn (Internal.StartStates.STARTSTATE x) =>
		 yybegin := x

fun lex () : Internal.result =
let fun continue() = lex() in
  let fun scan (s,AcceptingLeaves : Internal.yyfinstate list list,l,i0) =
	let fun action (i,nil) = raise LexError
	| action (i,nil::l) = action (i-1,l)
	| action (i,(node::acts)::l) =
		case node of
		    Internal.N yyk => 
			(let val yytext = substring(!yyb,i0,i-i0)
			     val yypos = i0+ !yygone
			open UserDeclarations Internal.StartStates
 in (yybufpos := i; case yyk of 

			(* Application actions *)

  100 => (lineNum := (!lineNum) + (String.size yytext); lex())
| 103 => (lex())
| 106 => (lex())
| 109 => (YYBEGIN INITIAL; lex())
| 111 => (lex())
| 114 => (lineNum := (!lineNum) + (String.size yytext); lex())
| 117 => (lex())
| 119 => (lex())
| 12 => (YID(!lineNum,!lineNum))
| 121 => (YYBEGIN INITIAL; lex())
| 2 => (lex())
| 35 => (YNUMB(!lineNum,!lineNum))
| 40 => (YINT(!lineNum,!lineNum))
| 45 => (YBINT(!lineNum,!lineNum))
| 5 => (lineNum := (!lineNum) + (String.size yytext); lex())
| 51 => (YSTRING(!lineNum,!lineNum))
| 54 => (YYBEGIN C; lex())
| 57 => (YDOTDOT(!lineNum,!lineNum))
| 59 => (YDOT(!lineNum,!lineNum))
| 61 => (YLPAR(!lineNum,!lineNum))
| 63 => (YRPAR(!lineNum,!lineNum))
| 65 => (YSEMI(!lineNum,!lineNum))
| 67 => (YCOMMA(!lineNum,!lineNum))
| 69 => (YCOLON(!lineNum,!lineNum))
| 71 => (YCARET(!lineNum,!lineNum))
| 73 => (YLBRA(!lineNum,!lineNum))
| 75 => (YRBRA(!lineNum,!lineNum))
| 77 => (YTILDE(!lineNum,!lineNum))
| 79 => (YLESS(!lineNum,!lineNum))
| 8 => (case find yytext of SOME v => v(!lineNum,!lineNum)
						  | _ => YID(!lineNum,!lineNum))
| 81 => (YEQUAL(!lineNum,!lineNum))
| 83 => (YGREATER(!lineNum,!lineNum))
| 85 => (YPLUS(!lineNum,!lineNum))
| 87 => (YMINUS(!lineNum,!lineNum))
| 89 => (YBAR(!lineNum,!lineNum))
| 91 => (YSTAR(!lineNum,!lineNum))
| 93 => (YSLASH(!lineNum,!lineNum))
| 95 => (YYBEGIN B; lex())
| 97 => (YILLCH(!lineNum,!lineNum))
| _ => raise Internal.LexerError

		) end )

	val {fin,trans} = Vector.sub(Internal.tab, s)
	val NewAcceptingLeaves = fin::AcceptingLeaves
	in if l = !yybl then
	     if trans = #trans(Vector.sub(Internal.tab,0))
	       then action(l,NewAcceptingLeaves
) else	    let val newchars= if !yydone then "" else yyinput 1024
